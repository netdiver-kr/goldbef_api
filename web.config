<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.webServer>
    <rewrite>
      <rules>
        <rule name="ReverseProxyToFastAPI" stopProcessing="true">
          <match url="(.*)" />
          <conditions>
            <add input="{REQUEST_URI}" pattern="^/(venv|__pycache__|\.git)" negate="true" />
          </conditions>
          <action type="Rewrite" url="http://localhost:8001/{R:1}" />
        </rule>
      </rules>
    </rewrite>
    <urlCompression doStaticCompression="false" doDynamicCompression="false" />
    <caching enabled="false" />
    <httpErrors errorMode="Detailed" />
  </system.webServer>
</configuration>
